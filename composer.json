{
    "name": "aosmak/laravel-layer-sdk",
    "type": "library",
    "description": "Powerful package that helps Larevel 5 projects to interact with Layer (http://layer.com).",
    "keywords": [
        "laravel 5",
        "php 7",
        "layer"
    ],
    "homepage": "https://github.com/andriiosmak/laravel-layer-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Osmak",
            "email": "aoworlda@meta.ua",
            "homepage": "https://github.com/andriiosmak",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0",
        "illuminate/support": "~5.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "sebastian/phpcpd": "*",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": {
        "psr-4": {
            "Aosmak\\Layer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aosmak\\Layer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0.1-dev"
        }
    }
}
