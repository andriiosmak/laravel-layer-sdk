{
    "name": "aosmak/laravel-layer-sdk",
    "type": "library",
    "description": "Library that allows PHP developers to make requests to Layer Server API.",
    "keywords": [
        "Laravel 5",
        "PHP 7.2",
        "Layer Server API",
        "Layer"
    ],
    "homepage": "https://github.com/andriiosmak/laravel-layer-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Osmak",
            "email": "andrii.osmak.dev@gmail.com",
            "homepage": "https://github.com/andriiosmak",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "~5.6",
        "illuminate/container": "~5.6",
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7",
        "internations/http-mock" : "~0.10",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "^3",
        "sebastian/phpcpd": "^4",
        "phpmd/phpmd": "^2"
    },
    "autoload": {
        "psr-4": {
            "Aosmak\\Laravel\\Layer\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aosmak\\Laravel\\Layer\\Sdk\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aosmak\\Laravel\\Layer\\Sdk\\LayerProvider"
            ]
        }
    }
}
