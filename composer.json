{
    "name": "aosmak/laravel-layer-sdk",
    "type": "library",
    "description": "Powerful package that helps Larevel 5 projects to interact with Layer (http://layer.com).",
    "keywords": [
        "laravel 5",
        "php 7.2",
        "layer"
    ],
    "homepage": "https://github.com/andriiosmak/laravel-layer-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Osmak",
            "email": "aoworlda@meta.ua",
            "homepage": "https://github.com/andriiosmak",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/support": "~5.5",
        "illuminate/container": "~5.5",
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6",
        "internations/http-mock" : "~0.10",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "^3",
        "sebastian/phpcpd": "^3",
        "phpmd/phpmd": "^2"
    },
    "autoload": {
        "psr-4": {
            "Aosmak\\Laravel\\Layer\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aosmak\\Laravel\\Layer\\Sdk\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0.1-dev"
        }
    }
}
